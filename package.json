{
  "name": "vscode-styled-jsx-languageserver",
  "displayName": "styled-jsx Language Server",
  "description": "Language server for styled-jsx",
  "version": "1.2.0",
  "author": {
    "name": "Alexis Rico",
    "email": "sferadev@gmail.com"
  },
  "license": "MIT",
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "publisher": "SferaDev",
  "engines": {
    "vscode": "^1.40.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Southclaws/vscode-styled-jsx-languageserver"
  },
  "bugs": {
    "url": "https://github.com/Southclaws/vscode-styled-jsx-languageserver/issues"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:javascriptreact",
    "onLanguage:javascript",
    "onLanguage:typescriptreact",
    "onLanguage:typescript",
    "onCommand:styledJsx.applyCodeAction"
  ],
  "main": "./client/out/extension",
  "scripts": {
    "compile": "npm run compile:client && npm run compile:server",
    "compile:client": "tsc -p ./client",
    "compile:server": "tsc -p ./server",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "npm run compile"
  },
  "dependencies": {
    "typescript": "^3.9.3",
    "vscode-css-languageservice": "^4.2.0",
    "vscode-languageclient": "^5.2.1",
    "vscode-languageserver": "^5.2.1",
    "vscode-languageserver-protocol": "^3.15.3",
    "vscode-nls": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^12.12.42",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "eslint": "^7.0.0",
    "eslint-plugin-react": "^7.20.0",
    "prettier": "^1.19.1",
    "vscode": "^1.1.37"
  }
}
